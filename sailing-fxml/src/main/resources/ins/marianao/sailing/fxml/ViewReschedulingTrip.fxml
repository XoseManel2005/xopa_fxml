<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="viewTripsDirectory" prefHeight="320.0" prefWidth="293.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ins.marianao.sailing.fxml.ControllerReschedulingTrip">
   <center>
		<AnchorPane prefHeight="311.0" prefWidth="293.0" styleClass="centerregion">
			<children>
            <HBox layoutX="29.0" layoutY="29.0" prefHeight="95.0" prefWidth="240.0" styleClass="input-group">
               <children>
                  <Label prefHeight="215.0" prefWidth="61.0" text="%fxml.text.viewTrips.reschedule.reason" />
                  <TextField fx:id="tfReason" prefWidth="150.0" />
               </children>
            </HBox>
            <HBox layoutX="27.0" layoutY="139.0" prefHeight="42.0" prefWidth="245.0" styleClass="input-group">
               <children>
						<Label prefHeight="20.0" prefWidth="61.0" text="%fxml.text.viewTrips.reschedule.date" />
                  <DatePicker fx:id="dpDate" minWidth="145.0" prefHeight="40.0" prefWidth="145.0" />
               </children>
            </HBox>
            <HBox layoutX="27.0" layoutY="200.0" prefHeight="27.0" prefWidth="212.0" styleClass="input-group">
               <children>
                  <Label prefHeight="20.0" prefWidth="61.0" text="%fxml.text.viewTrips.reschedule.departure" />
                  <ComboBox fx:id="cbDeparture" minWidth="103.0" prefWidth="103.0" />
               </children>
            </HBox>
            <Button fx:id="btnCancel" layoutX="14.0" layoutY="265.0" mnemonicParsing="false" onAction="#cancel" prefHeight="36.0" prefWidth="116.0" text="%fxml.text.viewTrips.done.cancel" />
            <Button fx:id="btnSubmit" layoutX="150.0" layoutY="265.0" mnemonicParsing="false" onAction="#submit" prefHeight="36.0" prefWidth="110.0" text="%fxml.text.viewTrips.reschedule.submit" />
            <Label layoutX="8.0" layoutY="5.0" prefHeight="0.0" text="%fxml.text.viewTrips.reschedule.text" />
			</children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
		</AnchorPane>
   </center>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</BorderPane>
